<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inputdatetimepicker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InputDateTimePicker">
        <activity
            android:name=".PickUpDeliveryActivity"
            android:label="@string/title_activity_pick_up_delivery"
            android:theme="@style/Theme.InputDateTimePicker.NoActionBar"></activity>
        <activity
            android:name=".PackagingActivity"
            android:label="@string/title_activity_packaging"
            android:theme="@style/Theme.InputDateTimePicker.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>